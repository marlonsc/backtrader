# .pylintrc for backtrader
# Disables 'no-member' (E1101) globally due to dynamic attributes injected by metaclasses
# (e.g., .p in MetaParams), which static analysis cannot detect.
[MESSAGES CONTROL]
disable=no-member,unexpected-keyword-arg,useless-import-alias,missing-function-docstring,no-member,unexpected-keyword-arg
